<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 1000 </nEvents>
  <nEvents_printout> 1 </nEvents_printout>
  
  <outputFilename>test_out</outputFilename>
  <JetScapeWriterHepMC> on </JetScapeWriterHepMC>
  <JetScapeWriterAscii> on </JetScapeWriterAscii>
  <JetScapeWriterFinalStatePartonsAscii> on </JetScapeWriterFinalStatePartonsAscii>
  <JetScapeWriterFinalStateHadronsAscii> on </JetScapeWriterFinalStateHadronsAscii>
  <final_state_writer_header_version> 3 </final_state_writer_header_version>
  <write_pthat> 1 </write_pthat>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>4</pTHatMin>
      <pTHatMax>-1</pTHatMax>
      <eCM>5020</eCM>
      <LinesToRead>
          PhaseSpace:bias2Selection = on
          PhaseSpace:bias2SelectionPow = 4.0
          PhaseSpace:bias2SelectionRef = 10
      </LinesToRead>

    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter> 
      <Q0> 1.0 </Q0>
      <in_vac> 1 </in_vac>
      <vir_factor> 0.25 </vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
      <name>colorless</name>
      <eCMforHadronization>2510</eCMforHadronization>
      <LinesToRead>111:mayDecay=off</LinesToRead>
  </JetHadronization>

  <PartonPrinter>
      <Status>off</Status>
      <FileName>./FinalPartonsInfo.dat</FileName>
  </PartonPrinter>
  
</jetscape>
